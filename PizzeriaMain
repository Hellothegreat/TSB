local Section = Money:CreateSection("Coming Soon!")

local Misc = Window:CreateTab("Misc", "settings") -- Title, Image

local Section = Misc:CreateSection("Gamepass")

local Toggle = Misc:CreateToggle({
   Name = "Get All Gamepasses",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(X)
   if X then
   game:GetService("Players").LocalPlayer.GamePasses["884091418"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884075540"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["721432459"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884159379"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884160292"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884188220"].Value = true 

game:GetService("Players").LocalPlayer.GamePasses["884212220"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884219302"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884260210"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884403116"].Value = true

game:GetService("Players").LocalPlayer.GamePasses["884320178"].Value = true

Rayfield:Notify({
   Title = "Gamepasses",
   Content = "Enabled",
   Duration = 3.5,
   Image = "check",
})

             else
game:GetService("Players").LocalPlayer.GamePasses["884091418"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884075540"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["721432459"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884159379"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884160292"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884188220"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884212220"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884219302"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884260210"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884403116"].Value = false

game:GetService("Players").LocalPlayer.GamePasses["884320178"].Value = false

Rayfield:Notify({
   Title = "Gamepasses",
   Content = "Disabled",
   Duration = 3.5,
   Image = "check",
})

           end
   end,
})



Rayfield:LoadConfiguration()
