local Money = Window:CreateTab("Money", "dollar-sign") -- Title, Image

local Button = Money:CreateButton({
   Name = "Add $1",
   Interact = 'Activate',
   Callback = function()
local args = {
    [1] = "Money",
    [2] = workspace.Restaurants.VanivratTSB4.Addments.Chairs:FindFirstChild("1-1"),
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.Clients:FireServer(unpack(args))

   end,
})

